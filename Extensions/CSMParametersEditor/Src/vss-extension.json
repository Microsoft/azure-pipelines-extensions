{
    "manifestVersion": 1.0,
    "extensionId": "vss-distributed-task-csm-parameters-editor",
    "name": "CSM Template Parameters Editor",
    "version": "0.0.1",
    "publisher": "ms",
    "public": false,
    "description": "i18n:Editors to help with complex distributed task inputs",
    "categories": [ "Integrate" ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "vss_default.png",
        "wide": "vss_wide.png"
    },
    "files": [
        {
            "path": "UIExtensions/gridEditor.html", "addressable": true
        },
        {
            "path": "UIExtensions/css", "addressable": true
        },
        {
            "path": "UIExtensions/sdk", "addressable": true
        },
        {
            "path": "UIExtensions/scripts", "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "azurerg-parameters-grid",
            "type": "ms.vss-distributed-task.task-input-editor",
            "targets": [ "ms.vss-distributed-task.task-input-editors" ],
            "properties": {
                "name":"CSM Template Parameters Editor",
                "uri": "UIExtensions/gridEditor.html",
                "order": "99"
            }
        }
    ]
}
